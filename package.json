{
  "name": "stratos",
  "version": "2.0.0",
  "description": "Stratos Console",
  "main": "index.js",
  "scripts": {
    "build-backend": "gulp build-backend",
    "build-backend-dev": "STRATOS_BACKEND_DEV=true npm run build-backend",
    "full-build-dev": "npm run build-dev; npm run build-backend-dev",
    "cf-get-backend-deps": "gulp cf-get-backend-deps",
    "cf-build-backend": "gulp cf-build-backend",
    "bosh-build-backend": "gulp bosh-build-backend",
    "deploy-cf": "gulp deploy:cf",
    "test-backend": "gulp test-backend",
    "test-backend-dev": "STRATOS_USE_GO_CONVEY=true gulp test-backend",
    "update-webdriver": "webdriver-manager update",
    "build": "ng build --prod",
    "build-cf": "node --max_old_space_size=1128 --optimize_for_size --gc_interval=100 node_modules/@angular/cli/bin/ng build --prod --aot=false --build-optimizer=false",
    "build-dev": "ng build --dev",
    "ng": "ng",
    "start": "npm run customize && ng serve",
    "start-high-mem": "npm run customize && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve",
    "start-prod": "ng serve --prod",
    "test": "ng test --code-coverage",
    "test-debug": "ng test --watch=true --source-map=true",
    "test-headless": "HEADLESS=true npm run test",
    "codecov": "codecov -f coverage/*.json",
    "lint": "ng lint",
    "sass-lint": "sass-lint -v",
    "e2e": "ng e2e",
    "e2e-dev": "ng e2e --dev-server-target= --base-url=https://127.0.0.1:4200",
    "e2e-local": "ng e2e --dev-server-target= --base-url=https://127.0.0.1",
    "headless-e2e": "xvfb-run --server-args='-screen 0 1920x1080x24' protractor ./protractor.conf.js",
    "climate": "codeclimate analyze $(git diff --name-only master)",
    "gate-check": "npm run lint && npm run test-headless",
    "customize": "gulp customize",
    "customize-default": "gulp customize-default",
    "customize-reset": "gulp customize-reset",
    "store-git-metadata": "gulp store-git-metadata",
    "postinstall": "npm run customize"
  },
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/animations": "6.0.3",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "6.0.3",
    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/platform-server": "6.0.3",
    "@angular/router": "6.0.3",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@swimlane/ngx-charts": "^8.1.0",
    "angular2-virtual-scroll": "^0.3.1",
    "core-js": "^2.5.7",
    "hammerjs": "^2.0.8",
    "js-yaml": "^3.11.0",
    "moment": "^2.22.1",
    "ngrx-store-logger": "^0.2.1",
    "normalizr": "^3.2.3",
    "reselect": "^3.0.1",
    "rxjs": "^6.2.0",
    "rxjs-spy": "^7.0.2",
    "rxjs-websockets": "git+https://github.com/ohjames/rxjs-websockets.git#28711f94f6988bd23d9a120061d6f9002961114b",
    "web-animations-js": "^2.3.1",
    "xterm": "^3.5.0",
    "zone.js": "~0.8.26"
  },
  "engines": {
    "node": "8.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.5",
    "@angular/cli": "6.0.5",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma": "^1.7.1",
    "@types/node": "^6.0.111",
    "@types/request": "^2.47.0",
    "codecov": "^3.0.2",
    "codelyzer": "~3.1.1",
    "delete": "^0.3.2",
    "fs-extra": "^3.0.1",
    "gulp": "^4.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "js-yaml": "~3.11.0",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "karma-spec-reporter": "0.0.31",
    "lodash": "^4.17.10",
    "mktemp": "^0.4.0",
    "ngrx-store-freeze": "^0.2.3",
    "protractor": "^5.3.2",
    "q": "^1.4.1",
    "replace-in-file": "^3.4.0",
    "request-promise-native": "^1.0.5",
    "rxjs-tslint": "^0.1.4",
    "sass-lint": "^1.12.1",
    "stratos-merge-dirs": "^0.2.3",
    "stratos-protractor-reporter": "^1.2.3",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "2.7.2"
  }
}
